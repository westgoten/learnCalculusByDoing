    <!-- Question 1 -->
    <string-array name="MAQ_stringArray1">
        <item>In which of the alternatives below does the limit exist?</item>
        <item>\\(\\lim\\limits_{x \\to 3^+} f(x) = 1\\) ; \\(\\lim\\limits_{x \\to 3^-} f(x) = 3\\)</item>
        <item>\\(\\lim\\limits_{x \\to 2^+} g(x) = 2\\) ; \\(\\lim\\limits_{x \\to 2^-} g(x) = 4\\)</item>
        <item>\\(\\lim\\limits_{x \\to 1^+} h(x) = 7\\) ; \\(\\lim\\limits_{x \\to 1^-} h(x) = 7\\)</item>
        <item>\\(\\lim\\limits_{x \\to 3^+} j(x) = 9\\) ; \\(\\lim\\limits_{x \\to 3^-} j(x) = 9\\)</item>
    </string-array>

    <!-- Question 2 -->
    <string-array name="MAQ_stringArray2">
        <item>In which of the alternatives below does continuity exist?</item>
        <item>\\(\\lim\\limits_{x \\to 2} f(x) = 2\\space;\\space f(2) = 7\\)</item>
        <item>\\(\\lim\\limits_{x \\to 4} f(x) = 11\\space;\\space f(4) = 11\\)</item>
        <item>\\(\\lim\\limits_{x \\to 7} f(x) = 3\\space;\\space f(7) = 6\\)</item>
        <item>\\(\\lim\\limits_{x \\to 9} f(x) = 9\\space;\\space f(9) = 9\\)</item>
    </string-array>

